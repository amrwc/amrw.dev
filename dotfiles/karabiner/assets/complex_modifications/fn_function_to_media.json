{
    "title": "fn_function_to_media",
    "rules": [
        {
            "description": "`fn` + `f[1-12]` -> media keys",
            "manipulators": [
                {
                    "from": {
                        "key_code": "f1",
                        "modifiers": {
                            "mandatory": ["fn"],
                            "optional": ["caps_lock"]
                        }
                    },
                    "to": [
                        {
                            "consumer_key_code": "display_brightness_decrement"
                        }
                    ],
                    "type": "basic"
                },
                {
